	ui = auto
[pull]
	rebase = false
[core]
	editor = nvim
[alias]
  s = status
  l = log
  pl = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' 
  co = checkout
  a = add
  aa = add .
  c = commit
	st = status
